# Общие настройки
NODE_ENV=production
TZ=Europe/Moscow

# Настройки PostgreSQL
PGHOST=localhost
PGUSER=your_username
PGPASSWORD=your_password
PGDATABASE=your_database
PGPORT=5432

# Настройки Telegram уведомлений
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_CHAT_ID=your_chat_id_here
TELEGRAM_ERRORS_ONLY=false

# Настройки режима работы
MANUAL_MODE=false
MANUAL_START_DATE=
MANUAL_END_DATE=
FORCE_OVERRIDE=false

# TopVisor API настройки
TOPVISOR_API_URL=https://api.topvisor.com/v2/json/get/positions_2/history
TOPVISOR_API_KEY=your_topvisor_api_key
TOPVISOR_USER_ID=your_topvisor_user_id

# WordStat API настройки (если используется)
WORDSTAT_API_KEY=your_wordstat_api_key
WORDSTAT_API_URL=https://api.direct.yandex.com/json/v5/keywordresearch

# Microsoft Clarity API настройки (если используется)  
CLARITY_API_KEY=your_clarity_api_key
CLARITY_PROJECT_ID=your_clarity_project_id
CLARITY_API_URL=https://www.clarity.ms/api

# Google Analytics 4 API настройки (если используется)
GA4_PROPERTY_ID=your_ga4_property_id
GA4_KEY_FILE_PATH=path/to/service-account-key.json
GA4_API_URL=https://analyticsdata.googleapis.com

# Google Search Console API настройки (если используется)
GSC_SITE_URL=https://example.com
GSC_KEY_FILE_PATH=path/to/service-account-key.json
GSC_API_URL=https://searchconsole.googleapis.com

# Яндекс.Метрика API настройки (если используется)
YANDEX_METRIKA_COUNTER_ID=your_counter_id
YANDEX_METRIKA_TOKEN=your_oauth_token
YANDEX_METRIKA_API_URL=https://api-metrica.yandex.net

# Настройки логирования
LOG_LEVEL=info
LOG_MAX_FILES=30
LOG_MAX_SIZE=10mb

# Настройки производительности
MAX_CONCURRENT_REQUESTS=5
REQUEST_DELAY_MS=1000
BATCH_SIZE=4